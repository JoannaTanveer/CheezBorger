<div class="header">

</div>

<h1>Cheez Borger</h1>

<div class='form'>
    <form>
       <label for="burger">Wat kine you like?</label>
        <input type="text" id='burger' name='burger'>
        <br>
        <button class='chomp' type='submit'>Chomp Chomp</button>
    </form>
</div>

<div class='toeat'>
    <h2>I can has</h2>

    <ul>
    {{#each name}}
        {{#unless this.eaten}}
       <li>
          <div class="nameId">{{this.id}}</div> . {{this.name}} <button data-id={{this.id}} type='submit' class='submit'>EAT ME!</button>

       </li>
       
        {{/unless}}
    {{/each}}
    </ul>

</div>

<div class='eaten'>
    <h2>Iz fav!</h2>

    <ul>
    {{#each name}}
        {{#if this.eaten}}
        <li>
          <div class="nameId">{{this.id}}</div> . {{this.name}} 

       </li>
        {{/if}}
    {{/each}}
    </ul>

</div>